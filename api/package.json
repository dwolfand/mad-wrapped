{
  "name": "mad-wrapped-api",
  "version": "1.0.0",
  "description": "MAD Wrapped API",
  "main": "dist/handler.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "dev": "AWS_PROFILE=personal serverless offline",
    "deploy": "serverless deploy --stage dev --aws-profile personal",
    "deploy:prod": "serverless deploy --stage production --aws-profile personal --verbose",
    "deploy-ci": "serverless deploy --stage production --verbose",
    "send-emails": "ts-node src/scripts/send_emails.ts",
    "send-notifications": "ts-node src/scripts/send_notifications.ts",
    "test-puppeteer": "ts-node src/test-puppeteer.ts"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.940.0",
    "@aws-sdk/client-ses": "^3.716.0",
    "@sentry/node": "^7.0.0",
    "@sentry/profiling-node": "^1.3.5",
    "@sparticuz/chromium": "^126.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/superagent": "^8.1.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "mongodb": "^6.12.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "puppeteer": "^24.31.0",
    "puppeteer-core": "^22.12.1",
    "serverless-http": "^3.1.1",
    "superagent": "^10.2.3",
    "twilio": "^5.11.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.10.0",
    "esbuild": "^0.17.19",
    "serverless": "^3.39.0",
    "serverless-domain-manager": "^8.0.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-esbuild": "^1.56.1",
    "serverless-offline": "^13.8.1",
    "serverless-plugin-chrome": "^1.0.0-55.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
